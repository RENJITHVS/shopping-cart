<section>
  <div class="container">
    <div class="row mt-4">
      <a href="/admin/addproduct" class="btn btn-success ml-auto btn-lg"> + </a>
    </div>
    <table class="table table-dark mt-5">
      <thead>
        <tr>
          <th scope="col">no</th>
          <th scope="col">title</th>
          <th scope="col">category</th>
          <th scope="col">description</th>
          <th scope="col">image</th>
        </tr>
      </thead>
      <tbody>
        {{#each products}}
        <tr>
          <th scope="row">1</th>
          <td>{{this.name}}</td>
          <td>{{this.category}}</td>
          <td>{{this.description}}</td>
          <td><img style="width: 150px; height:130px" src="{{this.image}}"></td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</section>